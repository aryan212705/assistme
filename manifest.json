{
  "manifest_version": 2,
  "name": "Assist Me",
  "version": "1.0",
  "description": "It helps makes browsing experience better by providing realtime information on the current webpage itself",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-3.4.1.min.js",
        "font_awesome_v5.js",
        "assistme_main.js"
      ],
      "css": [
        "assistme_main.css"
      ]
    }
  ],
  "permissions": [
    "https://api.dictionaryapi.dev/api/v2/entries/*",
    "http://localhost/*"
  ]
}